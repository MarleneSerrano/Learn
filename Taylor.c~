#include <stdio.h>
#include <stdlib.h>
#include <math.h>

#define F(x) (cos (x))

float recurcentral(float ta, float h, int n);

int main()
{
  float a, h, resultado;

  resultado = recursionCentral(ta, h, n);
    return 0;
}

float recurcentral(float ta, float h, int n)
{
  float resultado; 
  if(n>1)
    {
      float t = ta - h;
      return (F(ta) - F(t))/h;
    }
}
